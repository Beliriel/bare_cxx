#ifndef _UTILITY_HEADER_
#define _UTILITY_HEADER_

#include <type_traits>

namespace std {

template<class T>
constexpr remove_reference_t<T>&& move(T&& t)
{
  return static_cast<remove_reference_t<T>&&>(t); 
}

template<class T>
typename add_rvalue_reference<T>::type declval();

}

#endif

