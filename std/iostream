#ifndef _IOSTREAM_HEADER
#define _IOSTREAM_HEADER

#include <config.bare>
#include <ostream>

namespace _STD {

extern std::ostream cout;
extern std::wostream wcout;

extern std::ostream cerr;
extern std::wostream wcerr;

extern std::ostream clog;
extern std::wostream wclog;

}

#endif
