#ifndef _IOSTREAM_HEADER
#define _IOSTREAM_HEADER

#include <config.bare>
#include <ostream>

namespace std {

extern ostream cout;
extern wostream wcout;

extern ostream cerr;
extern wostream wcerr;

extern ostream clog;
extern wostream wclog;

}

#endif
